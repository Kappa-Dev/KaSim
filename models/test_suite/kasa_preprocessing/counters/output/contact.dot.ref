graph G{ 
subgraph cluster0 {
   0.0 [style = filled label = "x" shape =ellipse color = green size = "5"]
   0.1 [style = filled label = "c" shape =house color = grey size = "5"]
   0.2 [style = filled label = "d" shape =house color = grey size = "5"]
   0.3 [style = filled label = "x" shape =circle color = yellow size = "5"]
label =  "A";  shape = rectangle; color = blue
} ; 
subgraph cluster1 {
   1.0 [style = filled label = "c" shape =house color = grey size = "5"]
   1.1 [style = filled label = "x" shape =circle color = yellow size = "5"]
label =  "B";  shape = rectangle; color = blue
} ; 
0.3 -- 1.1
}
