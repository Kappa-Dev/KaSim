%def: "seed" "785872661"
%def: "syntaxVersion" "4"
%def: "dumpIfDeadlocked" "true"
%def: "maxConsecutiveClash" "2"
%def: "plotPeriod" "1" "events"
%def: "outputFileName" "data.csv"

%agent: A(x!z.A, y!x.B, z!x.A)
%agent: B(x!y.A)

%plot: [T]
'delete A' A(x!1/!.), -A(x?, y?, z!1) @ 1

%init: 1 A(x!1, y!2, z!.), A(x!., y!., z!1), B(x!2)

%mod: [E] = 1 do $STOP
