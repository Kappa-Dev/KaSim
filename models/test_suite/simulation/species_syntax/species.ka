%agent: A(a{u,p})
%agent: B(a,b,c)
%agent: C(c)

'ab' A(a[.]), B(a[.]) -> A(a[1]), B(a[1]) @ 0.01
'bb' B(b[.]), B(b[.]) -> B(b[1]), B(b[1]) @ 0.01
'bc' B(c[.]), C(c[.]) -> B(c[1]), C(c[1]) @ 0.01

%mod: [true] do $SPECIES_OF "species.ka" A(a[1]), B(a[1]) [true]
%mod: [true] do $SPECIES_OF "species2.ka" A(a[1]), B(a[1]), B(c[2]), C(c[2]) [true]

%init: 100 A(a{u})
%init: 100 B()
%init: 100 C()