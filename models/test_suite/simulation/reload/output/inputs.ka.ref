%def: "seed" "886009874"
%def: "dumpIfDeadlocked" "true"
%def: "maxConsecutiveClash" "2"
%def: "progressBarSize" "60"
%def: "progressBarSymbol" "#"
%def: "plotPeriod" "1" "t.u."
%def: "outputFileName" "data.csv"

%agent: A(s[s.B])
%agent: B(s[s.A])

%var:/*0*/ 'AB' |A(s[1]), B(s[1])|
%plot: [T]
%plot: AB

A(s[./1]), B(s[./1]) @ 0.001
A(s[1/.]), B(s[1/.]) @ 0.1

/*0*/%mod: alarm 1 [true] do $SNAPSHOT ("snap_".[E].".ka"); repeat [true]

%init: 100 A(s[.]), B(s[.])

%mod: [E] = 75 do $STOP;
