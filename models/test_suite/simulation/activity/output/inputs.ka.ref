%def: "seed" "226011309"
%def: "newSyntax" "true"
%def: "dumpIfDeadlocked" "true"
%def: "maxConsecutiveClash" "2"
%def: "plotPeriod" "20" "events"
%def: "outputFileName" "data.svg"

%agent: T()
%token: t

%var:/*0*/ 'nT' 10
%var:/*1*/ 't' |t|
%var:/*2*/ 'T' |T()|
%var:/*3*/ 'expo2_rate' |t|
%plot: [T]
%plot: t
%plot: T

'expo' T(), +T() @ 1
'expo2'  | -(1) t + 2 t @ expo2_rate


%init: nT T()
%init: (1 * 10) t

%mod: [E] = 1000 do $STOP
