%def: "seed" "253329392"
%def: "dumpIfDeadlocked" "true"
%def: "maxConsecutiveClash" "2"
%def: "plotPeriod" "1" "events"
%def: "outputFileName" "data.csv"

%agent: A(x!x.B)
%agent: B(x~0~1!x.A)
%agent: C()

%var:/*0*/ 'goal' 0
%var:/*1*/ 'bad' 1
%plot: [T]
A(x!_/!.) @ bad
A(x!_/!.), +C() @ goal
A(x!./!1), B(x!./!1) @ 1
A(x!./!1), B(x!./!1) @ 1
'GOAL' -A(x!1), -B(x~1!1), -C() @ 1

/*0*/%mod: [true] do $TRACK A(x!1), B(x~1!1), C() [true] repeat ([false])
/*1*/%mod: ([T] > 10) do $UPDATE bad 0; $UPDATE goal 1 repeat ([false])

%init: 1 A(x!1), B(x~1!1)
%init: 10 B(x~0!.)

%mod: [E] = 25 do $STOP
