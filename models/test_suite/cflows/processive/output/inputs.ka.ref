%def: "seed" "240056667"
%def: "syntaxVersion" "4"
%def: "dumpIfDeadlocked" "true"
%def: "maxConsecutiveClash" "2"
%def: "plotPeriod" "1" "events"
%def: "outputFileName" "data.csv"

%agent: A(x!x.K, y~u~p, z~u~p)
%agent: K(x!x.A)

%plot: [T]
A(x!./!1), K(x!./!1) @ 10
A(x!1/!.), K(x!1/!.) @ 1
A(x!1, y~u/~p!.), K(x!1) @ 1
A(x!1, z~u/~p!.), K(x!1) @ 1
'GOAL' A(y~p!., z~p!.) @ 10

/*0*/%mod: [true] do $TRACK A(y~p, z~p) [true] repeat ([false])

%init: 10 A(x!., y~u!., z~u!.)
%init: 10 K(x!.)

%mod: [E] = 30000 do $STOP
