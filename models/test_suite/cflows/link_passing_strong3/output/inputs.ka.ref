%def: "seed" "611463625"
%def: "dumpIfDeadlocked" "true"
%def: "maxConsecutiveClash" "2"
%def: "plotPeriod" "1" "events"
%def: "outputFileName" "data.csv"

%agent: A(x!x.E)
%agent: B(x)
%agent: C(x!x.E)
%agent: E(x!x.A!x.C)

%plot: [T]
-A(x!.), +B(x!.) @ 1
-B(x!.), +C(x!.) @ 1
-C(x!.), +A(x!.) @ 1
A(x!./!1), E(x!1/!1), C(x!1/!.) @ 1
E(x!./!1), C(x!./!1) @ 1
'GOAL' A(x!1/!.), E(x!1/!.) @ 1

/*0*/%mod: [true] do $TRACK A(x!1), E(x!1) [true] repeat ([false])

%init: 10 A(x!.)
%init: 10 E(x!.)

%mod: [E] = 280 do $STOP
