%def: "seed" "501792230"
%def: "dumpIfDeadlocked" "true"
%def: "maxConsecutiveClash" "2"
%def: "plotPeriod" "1" "events"
%def: "outputFileName" "data.csv"
%def: "traceFileName" "trace.json"

%agent: A(x~0~1!x.B)
%agent: B(x~0~1!x.A)
%agent: C()

%plot: [T]
+C() @ 1
A(x!_/!.) @ 1
A(x~0/~1!./!1), B(x!./!1) @ 1
A(x~1/~0!./!1), B(x!./!1) @ 1
'GOAL' -A(x~1!1), -B(x~1!1) @ 1

/*0*/%mod: [true] do $TRACK A(x~1!1), B(x~1!1) [true] repeat ([false])

%init: 1 A(x~0!1), B(x~1!1)
%init: 10 B(x~0!.)

%mod: [E] = 199 do $STOP
