%def: "seed" "924556145"
%def: "dumpIfDeadlocked" "true"
%def: "maxConsecutiveClash" "2"
%def: "progressBarSize" "60"
%def: "progressBarSymbol" "#"
%def: "plotPeriod" "1" "events"
%def: "outputFileName" "data.csv"

%agent: A(x[x.B])
%agent: B(x[x.A])

%plot: [T]
'a.b' A(x[./1]), B(x[./1]) @ 1
'B-' B(x[_])- @ 1
'B+' B(x[.])+ @ 1

/*0*/%mod: [true] do $TRACK A(x[1]), B(x[1]) [true]; repeat [false]

%init: 1000 B(x[.])
%init: 1 A(x[.])

%mod: [E] = 75 do $STOP;
