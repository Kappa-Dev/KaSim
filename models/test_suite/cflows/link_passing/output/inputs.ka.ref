%def: "seed" "924556145"
%def: "dumpIfDeadlocked" "true"
%def: "maxConsecutiveClash" "2"
%def: "plotPeriod" "1" "events"
%def: "outputFileName" "data.csv"

%agent: A(x!x.K, y!x.K, z!x.K)
%agent: K(x!x.A!y.A!z.A)

%plot: [T]
A(x[./1]), K(x[./1]) @ 2
A(x[1/.]), K(x[1/.]) @ 1
A(x[1/.] y[./1]), K(x[1/1]) @ 2
A(x[./1] y[1/.]), K(x[1/1]) @ 1
A(y[1/.] z[./1]), K(x[1/1]) @ 2
A(y[./1] z[1/.]), K(x[1/1]) @ 1
'GOAL' A(z[_]) @ 1

/*0*/%mod: [true] do $TRACK A(z[1]), K(x[1]) [true] repeat [false]

%init: 1 A(x[.] y[.] z[.])
%init: 1 K(x[.])

%mod: [E] = 10000 do $STOP
