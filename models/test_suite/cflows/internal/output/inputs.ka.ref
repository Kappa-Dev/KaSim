%def: "seed" "240056667"
%def: "dumpIfDeadlocked" "true"
%def: "maxConsecutiveClash" "2"
%def: "progressBarSize" "60"
%def: "progressBarSymbol" "#"
%def: "plotPeriod" "1" "events"
%def: "outputFileName" "data.csv"

%agent: A(x{a b c d e})

%plot: [T]
A(x{#/a}[.]) @ 3
A(x{#/b}[#]) @ 3
A(x{#/c}[#]) @ 3
A(x{#/d}[.]) @ 1
'GOAL' A(x{d}[#]) @ 1

/*0*/%mod: [true] do $TRACK A(x{d}) [true]; repeat [false]

%init: 1 A(x{a}[.])

%mod: [E] = 30000 do $STOP;
