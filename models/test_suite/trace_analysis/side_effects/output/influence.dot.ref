"0:C()->." -> "2:A(x[_],z{u}[.],y[_])->A(x[_],z{p}[.],y[_])" [label="[0*->0]" color="red" arrowhead="tee"];
"0:C()->." -> "3:C(a[.],b{u}[.])->C(a[.],b{p}[.])" [label="[0->0]" color="red" arrowhead="tee"];
