#Download sources/binaries at https://github.com/Kappa-Dev/KaSim
#
#Command line is: KaSa counters.ka --output-directory output -syntax 4
#
#This file contains the description of the influence map in dot.
#Please use graphviz (http://www.graphviz.org) or OmniGraffle to export it to a PDF
#
digraph G{
"0:A(x1{u})->A(x1{p},c+1)" [shape="box" fillcolor="#87ceeb" style=filled];
"1:A(x1{p},c>=1)->A(x1{u},c-1)" [shape="box" fillcolor="#87ceeb" style=filled];
"2:A(x2{u})->A(x2{p},c+1)" [shape="box" fillcolor="#87ceeb" style=filled];
"3:A(x2{p},c>=1)->A(x2{u},c-1)" [shape="box" fillcolor="#87ceeb" style=filled];
"4:A(x3{u})->A(x3{p},c+1)" [shape="box" fillcolor="#87ceeb" style=filled];
"5:A(x3{p},c>=1)->A(x3{u},c-1)" [shape="box" fillcolor="#87ceeb" style=filled];
"6:A(x4{u})->A(x4{p},c+1)" [shape="box" fillcolor="#87ceeb" style=filled];
"7:A(x4{p},c>=1)->A(x4{u},c-1)" [shape="box" fillcolor="#87ceeb" style=filled];
"8:A(x5{u})->A(x5{p},c+1)" [shape="box" fillcolor="#87ceeb" style=filled];
"9:A(x5{p},c>=1)->A(x5{u},c-1)" [shape="box" fillcolor="#87ceeb" style=filled];
"10:A(c>=3)->A(c-2,d+1)" [shape="box" fillcolor="#87ceeb" style=filled];
"11:A(d>=2)->A(c+2,d-1)" [shape="box" fillcolor="#87ceeb" style=filled];
}
