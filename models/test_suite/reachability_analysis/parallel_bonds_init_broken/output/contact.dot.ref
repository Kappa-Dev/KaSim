graph G{ 
subgraph cluster0 {
   0.0 [style = filled label = "z" shape =circle color = yellow size = "5"]
   0.1 [style = filled label = "t" shape =circle color = yellow size = "5"]
label =  "A";  shape = rectangle; color = red
} ; 
subgraph cluster1 {
   1.0 [style = filled label = "z" shape =circle color = yellow size = "5"]
   1.1 [style = filled label = "t" shape =circle color = yellow size = "5"]
label =  "B";  shape = rectangle; color = red
} ; 
0.0 -- 1.0 [color=red]
0.1 -- 1.1 [color=red]
}
