function main=main()
% command line: 
%      'KaDE' 'modifiers.ka' '-syntax' '4' '-d' 'output' '-l' '1' '-p''0.1'
%% THINGS THAT ARE KNOWN FROM KAPPA FILE AND KaSim OPTIONS:
%% 
%% init - the initial abundances of each species and token
%% tinit - the initial simulation time (likely 0)
%% tend - the final simulation time 
%% initialstep - initial time step at the beginning of numerical integration
%% maxstep - maximal time step for numerical integration
%% reltol - relative error tolerance;
%% abstol - absolute error tolerance;
%% period - the time period between points to return
%%
%% variables (init(i),y(i)) denote numbers of embeddings 
%% rule rates are corrected by the number of automorphisms in the lhs of rules


tinit=0;
tend=1;
initialstep=1e-05;
maxstep=0.02;
reltol=0.001;
abstol=0.001;
period=0.1;
nonnegative=false;

global nodevar
nodevar=6;
global max_stoc_coef
max_stoc_coef=0;
nvar=7;
nobs=3;
nrules=4;

global var
var=zeros(nvar,1);
global init
init=sparse(nodevar,1);
stoc=zeros(nrules,max_stoc_coef);
global k
global kd
global kun
global kdun
global stoc

k=zeros(nrules,1);
kd=zeros(nrules,1);
kun=zeros(nrules,1);
kdun=zeros(nrules,1);
global jacvar
jacvar=sparse(nvar,nodevar);
global jack
global jackd
global jackun
global jackund
global jacstoc

jack=zeros(nrules,nodevar);
jackd=zeros(nrules,nodevar);
jackun=zeros(nrules,nodevar);
jackund=zeros(nrules,nodevar);

t = 0.000000;

init(6)=t;
var(1)=10; % nT
init(2)=var(1); % T(x[.])
var(5)=init(2)+init(4)+init(5); % unbind_rate
var(4)=init(2); % T
init(1)=var(1); % U(x[.] y{u}[.])
var(3)=init(1)+init(3); % t
var(2)=init(1)+init(3)+init(4)+init(5); % qU

k(1)=1; % essai

uiIsOctave = false;
uiIsMatlab = false;
LIC = license('inuse');
for elem = 1:numel(LIC)
    envStr = LIC(elem).feature
    if strcmpi(envStr,'octave')
       LICname=envStr;
       uiIsOctave = true;
       break
    end
    if strcmpi(envStr,'matlab')
       LICname=envStr
       uiIsMatlab = true;
       break
    end
end


if nonnegative 
   options = odeset('RelTol', reltol, ...
                    'AbsTol', abstol, ...
                    'InitialStep', initialstep, ...
                    'MaxStep', maxstep, ...
                    'Jacobian', @ode_jacobian, ...
                   'NonNegative', [1:1:5]);
else
   options = odeset('RelTol', reltol, ...
                    'AbsTol', abstol, ...
                    'InitialStep', initialstep, ...
                    'MaxStep', maxstep, ...
                    'Jacobian', @ode_jacobian);
end


if nonnegative
   if uiIsMatlab
      soln =  ode15s(@ode_aux,[tinit tend],ode_init(),options);
      soln.y=soln.y';
      vt = soln.x;
      vy = soln.y;
   elseif uiIsOctave
      [vt,vy] = ode23s(@ode_aux,[tinit tend],ode_init(),options);
   end
else
   if uiIsMatlab
      soln =  ode15s(@ode_aux,[tinit tend],ode_init(),options);
      soln.y=soln.y';
      vt = soln.x;
      vy = soln.y;
   elseif uiIsOctave
      soln = ode2r(@ode_aux,[tinit tend],ode_init(),options);
      vt = soln.x;
      vy = soln.y;
   end
end;


nrows = length(vt);

tmp = zeros(nodevar,1);

n_points = floor ((tend-tinit)/period)+1;
t = linspace(tinit, tend, n_points);
obs = zeros(nrows,nobs);

for j=1:nrows
    for i=1:nodevar
        z(i)=vy(j,i);
    end
    h=ode_obs(z);
    for i=1:nobs
        obs(j,i)=h(i);
    end
end
if nobs==1
   y = interp1(vt, obs, t, 'pchip')';
else
   y = interp1(vt, obs, t, 'pchip');
end


filename = 'data.csv';
fid = fopen (filename,'w');
fprintf(fid,'# KaDE modifiers.ka -syntax 4 -d output -l 1 -p 0.1\n')
fprintf(fid,'# ')
fprintf(fid,'[T],')
fprintf(fid,'t,')
fprintf(fid,'T,')
fprintf(fid,'\n')
for j=1:n_points
    for i=1:nobs
        fprintf(fid,'%f,',y(j,i));
    end
    fprintf(fid,'\n');
end
fclose(fid);


end



function Init=ode_init()

global nodevar
global init
Init=zeros(nodevar,1);

Init(1) = init(1); % U(x[.] y{u}[.])
Init(2) = init(2); % T(x[.])
Init(3) = init(3); % U(x[.] y{p}[.])
Init(4) = init(4); % U(x[1] y{u}[.]), T(x[1])
Init(5) = init(5); % U(x[1] y{p}[.]), T(x[1])
Init(6) = init(6); % t
end


function dydt=ode_aux(t,y)

global nodevar
global max_stoc_coef
global var
global k
global kd
global kun
global kdun
global stoc

var(5)=y(2)+y(4)+y(5); % unbind_rate
var(4)=y(2); % T
var(3)=y(1)+y(3); % t
var(2)=y(1)+y(3)+y(4)+y(5); % qU

k(2)=var(1);
k(3)=var(2);
k(4)=var(5);

dydt=zeros(nodevar,1);

% rule    : essai
% reaction: U(x[.] y{p}[.]) -> T(x[.])

dydt(3)=dydt(3)-k(1)*y(3);
dydt(2)=dydt(2)+k(1)*y(3);

% rule    : bind
% reaction: U(x[.] y{p}[.]) + T(x[.]) -> U(x[1] y{p}[.]), T(x[1])

dydt(2)=dydt(2)-k(3)*y(2)*y(3);
dydt(3)=dydt(3)-k(3)*y(2)*y(3);
dydt(5)=dydt(5)+k(3)*y(2)*y(3);

% rule    : essai
% reaction: U(x[1] y{p}[.]), T(x[1]) -> T(x[.]) + T(x[.])

dydt(5)=dydt(5)-k(1)*y(5);
dydt(2)=dydt(2)+k(1)*y(5);
dydt(2)=dydt(2)+k(1)*y(5);

% rule    : unbind
% reaction: U(x[1] y{p}[.]), T(x[1]) -> U(x[.] y{p}[.]) + T(x[.])

dydt(5)=dydt(5)-k(4)*y(5);
dydt(2)=dydt(2)+k(4)*y(5);
dydt(3)=dydt(3)+k(4)*y(5);

% rule    : essai
% reaction: U(x[1] y{u}[.]), T(x[1]) -> T(x[.]) + T(x[.])

dydt(4)=dydt(4)-k(1)*y(4);
dydt(2)=dydt(2)+k(1)*y(4);
dydt(2)=dydt(2)+k(1)*y(4);

% rule    : unbind
% reaction: U(x[1] y{u}[.]), T(x[1]) -> U(x[.] y{u}[.]) + T(x[.])

dydt(4)=dydt(4)-k(4)*y(4);
dydt(2)=dydt(2)+k(4)*y(4);
dydt(1)=dydt(1)+k(4)*y(4);

% rule    : essai2
% reaction: U(x[1] y{u}[.]), T(x[1]) -> U(x[1] y{p}[.]), T(x[1])

dydt(4)=dydt(4)-k(2)*y(4);
dydt(5)=dydt(5)+k(2)*y(4);

% rule    : essai
% reaction: U(x[.] y{u}[.]) -> T(x[.])

dydt(1)=dydt(1)-k(1)*y(1);
dydt(2)=dydt(2)+k(1)*y(1);

% rule    : bind
% reaction: U(x[.] y{u}[.]) + T(x[.]) -> U(x[1] y{u}[.]), T(x[1])

dydt(2)=dydt(2)-k(3)*y(2)*y(1);
dydt(1)=dydt(1)-k(3)*y(2)*y(1);
dydt(4)=dydt(4)+k(3)*y(2)*y(1);

% rule    : essai2
% reaction: U(x[.] y{u}[.]) -> U(x[.] y{p}[.])

dydt(1)=dydt(1)-k(2)*y(1);
dydt(3)=dydt(3)+k(2)*y(1);
dydt(6)=1;

end


function jac=ode_jacobian(t,y)

global nodevar
global max_stoc_coef
global jacvar
global var
global k
global kd
global kun
global kdun
global stoc

global jack
global jackd
global jackun
global jackund
global jacstoc

var(5)=y(2)+y(4)+y(5); % unbind_rate
var(4)=y(2); % T
var(3)=y(1)+y(3); % t
var(2)=y(1)+y(3)+y(4)+y(5); % qU

k(2)=var(1);
k(3)=var(2);
k(4)=var(5);
jacvar(5,2)=1;
jacvar(5,4)=1;
jacvar(5,5)=1;
jacvar(4,2)=1;
jacvar(3,1)=1;
jacvar(3,3)=1;
jacvar(2,1)=1;
jacvar(2,3)=1;
jacvar(2,4)=1;
jacvar(2,5)=1;

jack(3,1)=jacvar(2,1);
jack(3,3)=jacvar(2,3);
jack(3,4)=jacvar(2,4);
jack(3,5)=jacvar(2,5);
jack(4,2)=jacvar(5,2);
jack(4,4)=jacvar(5,4);
jack(4,5)=jacvar(5,5);

jac=sparse(nodevar,nodevar);

% rule    : essai
% reaction: U(x[.] y{p}[.]) -> T(x[.])

jac(3,3)=jac(3,3)-k(1);
jac(2,3)=jac(2,3)+k(1);

% rule    : bind
% reaction: U(x[.] y{p}[.]) + T(x[.]) -> U(x[1] y{p}[.]), T(x[1])

jac(2,1)=jac(2,1)-jack(3,1)*y(2)*y(3);
jac(2,3)=jac(2,3)-jack(3,3)*y(2)*y(3);
jac(2,4)=jac(2,4)-jack(3,4)*y(2)*y(3);
jac(2,5)=jac(2,5)-jack(3,5)*y(2)*y(3);
jac(2,2)=jac(2,2)-k(3)*y(3);
jac(2,3)=jac(2,3)-k(3)*y(2);
jac(3,1)=jac(3,1)-jack(3,1)*y(2)*y(3);
jac(3,3)=jac(3,3)-jack(3,3)*y(2)*y(3);
jac(3,4)=jac(3,4)-jack(3,4)*y(2)*y(3);
jac(3,5)=jac(3,5)-jack(3,5)*y(2)*y(3);
jac(3,2)=jac(3,2)-k(3)*y(3);
jac(3,3)=jac(3,3)-k(3)*y(2);
jac(5,1)=jac(5,1)+jack(3,1)*y(2)*y(3);
jac(5,3)=jac(5,3)+jack(3,3)*y(2)*y(3);
jac(5,4)=jac(5,4)+jack(3,4)*y(2)*y(3);
jac(5,5)=jac(5,5)+jack(3,5)*y(2)*y(3);
jac(5,2)=jac(5,2)+k(3)*y(3);
jac(5,3)=jac(5,3)+k(3)*y(2);

% rule    : essai
% reaction: U(x[1] y{p}[.]), T(x[1]) -> T(x[.]) + T(x[.])

jac(5,5)=jac(5,5)-k(1);
jac(2,5)=jac(2,5)+k(1);
jac(2,5)=jac(2,5)+k(1);

% rule    : unbind
% reaction: U(x[1] y{p}[.]), T(x[1]) -> U(x[.] y{p}[.]) + T(x[.])

jac(5,2)=jac(5,2)-jack(4,2)*y(5);
jac(5,4)=jac(5,4)-jack(4,4)*y(5);
jac(5,5)=jac(5,5)-jack(4,5)*y(5);
jac(5,5)=jac(5,5)-k(4);
jac(2,2)=jac(2,2)+jack(4,2)*y(5);
jac(2,4)=jac(2,4)+jack(4,4)*y(5);
jac(2,5)=jac(2,5)+jack(4,5)*y(5);
jac(2,5)=jac(2,5)+k(4);
jac(3,2)=jac(3,2)+jack(4,2)*y(5);
jac(3,4)=jac(3,4)+jack(4,4)*y(5);
jac(3,5)=jac(3,5)+jack(4,5)*y(5);
jac(3,5)=jac(3,5)+k(4);

% rule    : essai
% reaction: U(x[1] y{u}[.]), T(x[1]) -> T(x[.]) + T(x[.])

jac(4,4)=jac(4,4)-k(1);
jac(2,4)=jac(2,4)+k(1);
jac(2,4)=jac(2,4)+k(1);

% rule    : unbind
% reaction: U(x[1] y{u}[.]), T(x[1]) -> U(x[.] y{u}[.]) + T(x[.])

jac(4,2)=jac(4,2)-jack(4,2)*y(4);
jac(4,4)=jac(4,4)-jack(4,4)*y(4);
jac(4,5)=jac(4,5)-jack(4,5)*y(4);
jac(4,4)=jac(4,4)-k(4);
jac(2,2)=jac(2,2)+jack(4,2)*y(4);
jac(2,4)=jac(2,4)+jack(4,4)*y(4);
jac(2,5)=jac(2,5)+jack(4,5)*y(4);
jac(2,4)=jac(2,4)+k(4);
jac(1,2)=jac(1,2)+jack(4,2)*y(4);
jac(1,4)=jac(1,4)+jack(4,4)*y(4);
jac(1,5)=jac(1,5)+jack(4,5)*y(4);
jac(1,4)=jac(1,4)+k(4);

% rule    : essai2
% reaction: U(x[1] y{u}[.]), T(x[1]) -> U(x[1] y{p}[.]), T(x[1])

jac(4,4)=jac(4,4)-k(2);
jac(5,4)=jac(5,4)+k(2);

% rule    : essai
% reaction: U(x[.] y{u}[.]) -> T(x[.])

jac(1,1)=jac(1,1)-k(1);
jac(2,1)=jac(2,1)+k(1);

% rule    : bind
% reaction: U(x[.] y{u}[.]) + T(x[.]) -> U(x[1] y{u}[.]), T(x[1])

jac(2,1)=jac(2,1)-jack(3,1)*y(2)*y(1);
jac(2,3)=jac(2,3)-jack(3,3)*y(2)*y(1);
jac(2,4)=jac(2,4)-jack(3,4)*y(2)*y(1);
jac(2,5)=jac(2,5)-jack(3,5)*y(2)*y(1);
jac(2,2)=jac(2,2)-k(3)*y(1);
jac(2,1)=jac(2,1)-k(3)*y(2);
jac(1,1)=jac(1,1)-jack(3,1)*y(2)*y(1);
jac(1,3)=jac(1,3)-jack(3,3)*y(2)*y(1);
jac(1,4)=jac(1,4)-jack(3,4)*y(2)*y(1);
jac(1,5)=jac(1,5)-jack(3,5)*y(2)*y(1);
jac(1,2)=jac(1,2)-k(3)*y(1);
jac(1,1)=jac(1,1)-k(3)*y(2);
jac(4,1)=jac(4,1)+jack(3,1)*y(2)*y(1);
jac(4,3)=jac(4,3)+jack(3,3)*y(2)*y(1);
jac(4,4)=jac(4,4)+jack(3,4)*y(2)*y(1);
jac(4,5)=jac(4,5)+jack(3,5)*y(2)*y(1);
jac(4,2)=jac(4,2)+k(3)*y(1);
jac(4,1)=jac(4,1)+k(3)*y(2);

% rule    : essai2
% reaction: U(x[.] y{u}[.]) -> U(x[.] y{p}[.])

jac(1,1)=jac(1,1)-k(2);
jac(3,1)=jac(3,1)+k(2);
end


function obs=ode_obs(y)

global nobs
global var
obs=zeros(nobs,1);

t = y(6);
var(5)=y(2)+y(4)+y(5); % unbind_rate
var(4)=y(2); % T
var(3)=y(1)+y(3); % t
var(2)=y(1)+y(3)+y(4)+y(5); % qU

obs(1)=t; % [T]
obs(2)=var(3); % t
obs(3)=var(4); % T

end


main();

