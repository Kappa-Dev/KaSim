function main=main()
% command line: 
%      'KaDE' 'tokens4.ka' '-syntax' '4' '-d' 'output' '-l' '1' '-p''0.1'
%% THINGS THAT ARE KNOWN FROM KAPPA FILE AND KaSim OPTIONS:
%% 
%% init - the initial abundances of each species and token
%% tinit - the initial simulation time (likely 0)
%% tend - the final simulation time 
%% initialstep - initial time step at the beginning of numerical integration
%% maxstep - maximal time step for numerical integration
%% reltol - relative error tolerance;
%% abstol - absolute error tolerance;
%% period - the time period between points to return
%%
%% variables (init(i),y(i)) denote numbers of embeddings 
%% rule rates are corrected by the number of automorphisms in the lhs of rules


tinit=0;
tend=1;
initialstep=1e-05;
maxstep=0.02;
reltol=0.001;
abstol=0.001;
period=0.1;
nonnegative=false;

global nodevar
nodevar=4;
global max_stoc_coef
max_stoc_coef=3;
nvar=7;
nobs=3;
nrules=3;

global var
var=zeros(nvar,1);
global init
init=sparse(nodevar,1);
stoc=zeros(nrules,max_stoc_coef);
global k
global kd
global kun
global kdun
global stoc

k=zeros(nrules,1);
kd=zeros(nrules,1);
kun=zeros(nrules,1);
kdun=zeros(nrules,1);
global jacvar
jacvar=sparse(nvar,nodevar);
global jack
global jackd
global jackun
global jackund
global jacstoc

jack=zeros(nrules,nodevar);
jackd=zeros(nrules,nodevar);
jackun=zeros(nrules,nodevar);
jackund=zeros(nrules,nodevar);

t = 0.000000;

init(4)=t;
init(2)=1;
var(5)=init(2); % expo3_rate
var(4)=init(2)+init(3); % expo2_rate
var(1)=10; % nT
init(1)=var(1); % T()
var(3)=init(1); % T
var(2)=init(2); % t

k(1)=1; % expo
stoc(2,2)=2;
stoc(3,3)=2.2;
stoc(3,2)=-2;

uiIsOctave = false;
uiIsMatlab = false;
LIC = license('inuse');
for elem = 1:numel(LIC)
    envStr = LIC(elem).feature
    if strcmpi(envStr,'octave')
       LICname=envStr;
       uiIsOctave = true;
       break
    end
    if strcmpi(envStr,'matlab')
       LICname=envStr
       uiIsMatlab = true;
       break
    end
end


if nonnegative 
   options = odeset('RelTol', reltol, ...
                    'AbsTol', abstol, ...
                    'InitialStep', initialstep, ...
                    'MaxStep', maxstep, ...
                    'Jacobian', @ode_jacobian, ...
                   'NonNegative', [1:1:1]);
else
   options = odeset('RelTol', reltol, ...
                    'AbsTol', abstol, ...
                    'InitialStep', initialstep, ...
                    'MaxStep', maxstep, ...
                    'Jacobian', @ode_jacobian);
end


if nonnegative
   if uiIsMatlab
      soln =  ode15s(@ode_aux,[tinit tend],ode_init(),options);
      soln.y=soln.y';
      vt = soln.x;
      vy = soln.y;
   elseif uiIsOctave
      [vt,vy] = ode23s(@ode_aux,[tinit tend],ode_init(),options);
   end
else
   if uiIsMatlab
      soln =  ode15s(@ode_aux,[tinit tend],ode_init(),options);
      soln.y=soln.y';
      vt = soln.x;
      vy = soln.y;
   elseif uiIsOctave
      soln = ode2r(@ode_aux,[tinit tend],ode_init(),options);
      vt = soln.x;
      vy = soln.y;
   end
end;


nrows = length(vt);

tmp = zeros(nodevar,1);

n_points = floor ((tend-tinit)/period)+1;
t = linspace(tinit, tend, n_points);
obs = zeros(nrows,nobs);

for j=1:nrows
    for i=1:nodevar
        z(i)=vy(j,i);
    end
    h=ode_obs(z);
    for i=1:nobs
        obs(j,i)=h(i);
    end
end
if nobs==1
   y = interp1(vt, obs, t, 'pchip')';
else
   y = interp1(vt, obs, t, 'pchip');
end


filename = 'data.csv';
fid = fopen (filename,'w');
fprintf(fid,'# KaDE tokens4.ka -syntax 4 -d output -l 1 -p 0.1\n')
fprintf(fid,'# ')
fprintf(fid,'[T],')
fprintf(fid,'t,')
fprintf(fid,'T,')
fprintf(fid,'\n')
for j=1:n_points
    for i=1:nobs
        fprintf(fid,'%f,',y(j,i));
    end
    fprintf(fid,'\n');
end
fclose(fid);


end



function Init=ode_init()

global nodevar
global init
Init=zeros(nodevar,1);

Init(1) = init(1); % T()
Init(2) = init(2); 
Init(3) = init(3); 
Init(4) = init(4); % t
end


function dydt=ode_aux(t,y)

global nodevar
global max_stoc_coef
global var
global k
global kd
global kun
global kdun
global stoc

var(5)=y(2); % expo3_rate
var(4)=y(2)+y(3); % expo2_rate
var(3)=y(1); % T
var(2)=y(2); % t

stoc(2,1)=(-y(2));
k(2)=var(4);
stoc(3,1)=(-(y(1)+y(2)+var(1)));
k(3)=var(5);

dydt=zeros(nodevar,1);

% rule    : expo
% reaction: T() -> T() + T()

dydt(1)=dydt(1)-k(1)*y(1);
dydt(1)=dydt(1)+k(1)*y(1);
dydt(1)=dydt(1)+k(1)*y(1);

% rule    : expo3
% reaction:  ->  | 2.2 t, -(2) u, -((|T()| + (|t| + nT))) t

dydt(2)=dydt(2)+k(3)*stoc(3,1);
dydt(3)=dydt(3)+k(3)*stoc(3,2);
dydt(2)=dydt(2)+k(3)*stoc(3,3);

% rule    : expo2
% reaction:  ->  | 2 t, -(|t|) t

dydt(2)=dydt(2)+k(2)*stoc(2,1);
dydt(2)=dydt(2)+k(2)*stoc(2,2);
dydt(4)=1;

end


function jac=ode_jacobian(t,y)

global nodevar
global max_stoc_coef
global jacvar
global var
global k
global kd
global kun
global kdun
global stoc

global jack
global jackd
global jackun
global jackund
global jacstoc

var(5)=y(2); % expo3_rate
var(4)=y(2)+y(3); % expo2_rate
var(3)=y(1); % T
var(2)=y(2); % t

stoc(2,1)=(-y(2));
k(2)=var(4);
stoc(3,1)=(-(y(1)+y(2)+var(1)));
k(3)=var(5);
jacvar(5,2)=1;
jacvar(4,2)=1;
jacvar(4,3)=1;
jacvar(3,1)=1;
jacvar(2,2)=1;

jacstoc(2,1,2)=-1;
jack(2,2)=jacvar(4,2);
jack(2,3)=jacvar(4,3);
jacstoc(3,1,1)=(-(1+jacvar(1,1)));
jacstoc(3,1,2)=(-(1+jacvar(1,2)));
jack(3,2)=jacvar(5,2);

jac=sparse(nodevar,nodevar);

% rule    : expo
% reaction: T() -> T() + T()

jac(1,1)=jac(1,1)-k(1);
jac(1,1)=jac(1,1)+k(1);
jac(1,1)=jac(1,1)+k(1);

% rule    : expo3
% reaction:  ->  | 2.2 t, -(2) u, -((|T()| + (|t| + nT))) t

jac(2,2)=jac(2,2)+jack(3,2)*stoc(3,1);
jac(2,1)=jac(2,1)+k(3)*jacstoc(3,1,1);
jac(2,2)=jac(2,2)+k(3)*jacstoc(3,1,2);
jac(3,2)=jac(3,2)+jack(3,2)*stoc(3,2);
jac(2,2)=jac(2,2)+jack(3,2)*stoc(3,3);

% rule    : expo2
% reaction:  ->  | 2 t, -(|t|) t

jac(2,2)=jac(2,2)+jack(2,2)*stoc(2,1);
jac(2,3)=jac(2,3)+jack(2,3)*stoc(2,1);
jac(2,2)=jac(2,2)+k(2)*jacstoc(2,1,2);
jac(2,2)=jac(2,2)+jack(2,2)*stoc(2,2);
jac(2,3)=jac(2,3)+jack(2,3)*stoc(2,2);
end


function obs=ode_obs(y)

global nobs
global var
obs=zeros(nobs,1);

t = y(4);
var(5)=y(2); % expo3_rate
var(4)=y(2)+y(3); % expo2_rate
var(3)=y(1); % T
var(2)=y(2); % t

obs(1)=t; % [T]
obs(2)=var(2); % t
obs(3)=var(3); % T

end


main();

