begin model
begin parameters
    Lig_tot  6.0e3
    Rec_tot  4.0e2
    Lyn_tot  2.8e1
    Fyn_tot  4.0e2
    Syk_tot  4.0e2

    kp1  1.32845238e-7
    km1  0
    kp2  2.5e-1
    km2  0
    kpL  5e-2
    kmL  20
    kpLs  5e-2
    kmLs  0.12
    kpF  5e-2
    kmF  20
    kpS  6e-2
    kmS  0.13
    kpSs  6e-2
    kmSs  0.13
    pLb  30
    pLbs  100
    pLg    1
    pLgs    3
    pLS   30
    pLSs  100
    pSS  100
    pSSs  200
    pLF   30
    pLFs  100
    dm   20
    dc   20
end parameters

begin seed species
    Lig(l1,l2,l3)      Lig_tot
    Lyn(U,SH2,a~Y)     Lyn_tot
    Fyn(SH2,a~Y,t~Y)   Fyn_tot
    Syk(tSH2,l~Y,a~Y)  Syk_tot
    Rec(a,b~Y,g~Y)     Rec_tot
end seed species

begin reaction rules
    # Ligand-receptor binding
    Rec(a) + Lig(l1,l2,l3) <-> Rec(a!1).Lig(l1!1,l2,l3) kp1, km1
    Rec(a) + Lig(l1,l2!_,l3) <-> Rec(a!1).Lig(l1!1,l2!_,l3) kp1, km1
    Rec(a) + Lig(l1,l2,l3!_) <-> Rec(a!1).Lig(l1!1,l2,l3!_) kp1, km1

    Rec(a) + Lig(l2,l1,l3) <-> Rec(a!1).Lig(l2!1,l1,l3) kp1, km1
    Rec(a) + Lig(l2,l1!_,l3) <-> Rec(a!1).Lig(l2!1,l1!_,l3) kp1, km1
    Rec(a) + Lig(l2,l1,l3!_) <-> Rec(a!1).Lig(l2!1,l1,l3!_) kp1, km1

    Rec(a) + Lig(l3,l1,l2) <-> Rec(a!1).Lig(l3!1,l1,l2) kp1, km1
    Rec(a) + Lig(l3,l1!_,l2) <-> Rec(a!1).Lig(l3!1,l1!_,l2) kp1, km1
    Rec(a) + Lig(l3,l1,l2!_) <-> Rec(a!1).Lig(l3!1,l1,l2!_) kp1, km1

    # Receptor-aggregation
    Rec(a) + Lig(l1,l2!+,l3) <-> Rec(a!2).Lig(l1!2,l2!+,l3)  kp2, km2
    Rec(a) + Lig(l2,l1!+,l3) <-> Rec(a!2).Lig(l2!2,l1!+,l3)  kp2, km2
    Rec(a) + Lig(l3,l1!+,l2) <-> Rec(a!2).Lig(l3!2,l1!+,l2)  kp2, km2

    Rec(a) + Lig(l1,l2!+,l3!+) <-> Rec(a!2).Lig(l1!2,l2!+,l3!+)  kp2,km2
    Rec(a) + Lig(l2,l1!+,l3!+) <-> Rec(a!2).Lig(l2!2,l1!+,l3!+)  kp2,km2
    Rec(a) + Lig(l3,l1!+,l2!+) <-> Rec(a!2).Lig(l3!2,l1!+,l2!+)  kp2,km2

    Rec(a) + Lig(l1,l2,l3!+) <-> Rec(a!2).Lig(l1!2,l2,l3!+)  kp2,km2
    Rec(a) + Lig(l2,l1,l3!+) <-> Rec(a!2).Lig(l2!2,l1,l3!+)  kp2,km2
    Rec(a) + Lig(l3,l1,l2!+) <-> Rec(a!2).Lig(l3!2,l1,l2!+)  kp2,km2

    # Constitutive Lyn-receptor binding
    Rec(b~Y) + Lyn(U,SH2) <-> Rec(b~Y!1).Lyn(U!1,SH2) kpL, kmL

    # Transphosphorylation of beta by constitutive Lyn

    #1 Third site is free

    Lig(l1!1,l2!2,l3).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~Y) -> Lig(l1!1,l2!2,l3).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~pY) pLb

    Lig(l2!1,l1!2,l3).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~Y) -> Lig(l2!1,l1!2,l3).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~pY) pLb

    Lig(l3!1,l1!2,l2).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~Y) -> Lig(l3!1,l1!2,l2).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~pY) pLb

    Lig(l1!1,l2,l3!2).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~Y) -> Lig(l1!1,l2,l3!2).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~pY) pLb

    Lig(l2!1,l1,l3!2).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~Y) -> Lig(l2!1,l1,l3!2).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~pY) pLb

    Lig(l3!1,l1,l2!2).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~Y) -> Lig(l3!1,l1,l2!2).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~pY) pLb

    #2 Second and third sites have the same state

    Lig(l1!1,l2!2,l3!4).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~Y).Rec(a!4,b~Y!5).Lyn(U!5,SH2) -> Lig(l1!1,l2!2,l3!4).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~pY).Rec(a!4,b~Y!5).Lyn(U!5,SH2) pLb

    Lig(l2!1,l1!2,l3!4).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~Y).Rec(a!4,b~Y!5).Lyn(U!5,SH2) -> Lig(l2!1,l1!2,l3!4).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~pY).Rec(a!4,b~Y!5).Lyn(U!5,SH2) pLb

    Lig(l3!1,l1!2,l2!4).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~Y).Rec(a!4,b~Y!5).Lyn(U!5,SH2) -> Lig(l3!1,l1!2,l2!4).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~pY).Rec(a!4,b~Y!5).Lyn(U!5,SH2) pLb

    #3 Third site is bound to a free receptor

    Lig(l1!1,l2!2,l3!4).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~Y).Rec(a!4,b) -> Lig(l1!1,l2!2,l3!4).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~pY).Rec(a!4,b) pLb

    Lig(l2!1,l1!2,l3!4).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~Y).Rec(a!4,b) -> Lig(l2!1,l1!2,l3!4).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~pY).Rec(a!4,b) pLb

    Lig(l3!1,l1!2,l2!4).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~Y).Rec(a!4,b)-> Lig(l3!1,l1!2,l2!4).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~pY).Rec(a!4,b) pLb

    Lig(l1!1,l2!2,l3!4).Rec(a!2,b).Rec(a!1,b~Y).Rec(a!4,b~Y!5).Lyn(U!5,SH2) -> Lig(l1!1,l2!2,l3!4).Rec(a!2,b).Rec(a!1,b~pY).Rec(a!4,b~Y!5).Lyn(U!5,SH2) pLb

    Lig(l2!1,l1!2,l3!4).Rec(a!2,b).Rec(a!1,b~Y).Rec(a!4,b~Y!5).Lyn(U!5,SH2) -> Lig(l2!1,l1!2,l3!4).Rec(a!2,b).Rec(a!1,b~pY).Rec(a!4,b~Y!5).Lyn(U!5,SH2) pLb

    Lig(l3!1,l1!2,l2!4).Rec(a!2,b).Rec(a!1,b~Y).Rec(a!4,b~Y!5).Lyn(U!5,SH2) -> Lig(l3!1,l1!2,l2!4).Rec(a!2,b).Rec(a!1,b~pY).Rec(a!4,b~Y!5).Lyn(U!5,SH2) pLb

    #4 Third site is bound to a bound but phosphorylated receptor

    Lig(l1!1,l2!2,l3!4).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~Y).Rec(a!4,b~pY!+) -> Lig(l1!1,l2!2,l3!4).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~pY).Rec(a!4,b~pY!+) pLb

    Lig(l2!1,l1!2,l3!4).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~Y).Rec(a!4,b~pY!+) -> Lig(l2!1,l1!2,l3!4).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~pY).Rec(a!4,b~pY!+) pLb

    Lig(l3!1,l1!2,l2!4).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~Y).Rec(a!4,b~pY!+)-> Lig(l3!1,l1!2,l2!4).Lyn(U!3,SH2).Rec(a!2,b~Y!3).Rec(a!1,b~pY).Rec(a!4,b~pY!+) pLb

    Lig(l1!1,l2!2,l3!4).Rec(a!2,b~pY!+).Rec(a!1,b~Y).Rec(a!4,b~Y!5).Lyn(U!5,SH2) -> Lig(l1!1,l2!2,l3!4).Rec(a!2,b~pY!+).Rec(a!1,b~pY).Rec(a!4,b~Y!5).Lyn(U!5,SH2) pLb

    Lig(l2!1,l1!2,l3!4).Rec(a!2,b~pY!+).Rec(a!1,b~Y).Rec(a!4,b~Y!5).Lyn(U!5,SH2) -> Lig(l2!1,l1!2,l3!4).Rec(a!2,b~pY!+).Rec(a!1,b~pY).Rec(a!4,b~Y!5).Lyn(U!5,SH2) pLb

    Lig(l3!1,l1!2,l2!4).Rec(a!2,b~pY!+).Rec(a!1,b~Y).Rec(a!4,b~Y!5).Lyn(U!5,SH2) -> Lig(l3!1,l1!2,l2!4).Rec(a!2,b~pY!+).Rec(a!1,b~pY).Rec(a!4,b~Y!5).Lyn(U!5,SH2) pLb

